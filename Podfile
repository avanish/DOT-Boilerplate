platform :ios, '9.0'

target 'MVVM Boilerplate' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MVVM Boilerplate

  pod 'Alamofire'
  pod 'Kingfisher'
  pod 'ObjectMapper'

  target 'MVVM BoilerplateTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MVVM BoilerplateUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
